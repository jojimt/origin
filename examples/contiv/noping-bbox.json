{
  "kind": "Pod",
  "apiVersion": "v1",
  "metadata": {
    "name": "noping",
    "creationTimestamp": null,
    "labels": {
      "name": "noping",
      "io.contiv.network": "user-net2",
      "io.contiv.net-group": "noping-epg"
    }
  },
  "spec": {
